<h1>Friends#index</h1>
<p>Find me in app/views/friends/index.html.erb</p>

<% if current_user == @user %>
    <%= render_users_relationships(@pending_incoming_fr_users,
    common_friends_precomputed:@common_friends_precomputed,
    interactions_precomputed:@interactions_precomputed,
    section_heading:"Friends Invitations") %>

    <%= render_users_relationships(@pending_outgoing_fr_users,
    common_friends_precomputed:@common_friends_precomputed,
    interactions_precomputed:@interactions_precomputed,
    section_heading:"Friends Pending") %>
<% end %>

<%= render_users_relationships(@user_friends,
common_friends_precomputed:@common_friends_precomputed,
interactions_precomputed:@interactions_precomputed,
section_heading: "Friends") %>
